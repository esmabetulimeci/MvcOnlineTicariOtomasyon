@using MvcOnlineTicariOtomasyon.Models.Sınıflar
@model List<Urun>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Quicksand:300,400,500,700,900" rel="stylesheet">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/fonts/icomoon/style.css">

    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/magnific-popup.css">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/jquery-ui.css">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/owl.theme.default.min.css">

    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/bootstrap-datepicker.css">

    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/fonts/flaticon/font/flaticon.css">

    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/aos.css">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/jquery.scrollbar.css">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/fancybox.min.css">
    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/swiper.min.css">

    <link rel="stylesheet" href="~/MvcOnlineTicariOtomasyon/thumber/css/style.css">

</head>
<body>


    <div class="site-wrap">
        <br />
        <main class="main-content">
           
            <div class="container-fluid photos">
                <div class="row align-items-stretch">
                    @foreach (var item in Model)
                    {
                        <div class="col-6 col-md-6 col-lg-4" data-aos="fade-up">

                            <a href="@item.UrunGorsel"
                               class="d-block photo-item" data-fancybox="gallery">
                                <img src="@item.UrunGorsel" alt="Image" class="img-fluid">
                                <div class="photo-text-more">
                                    <span class="icon icon-search"></span>
                                </div>
                            </a>
                            
                        </div>
                    }
                    </div>
              
            </div>
          
        </main>

    </div> <!-- .site-wrap -->


   
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/jquery-3.3.1.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/jquery-ui.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/popper.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/bootstrap.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/owl.carousel.min.js"></script>
    <!-- <script src="js/jquery.stellar.min.js"></script> -->
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/jquery.countdown.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/jquery.magnific-popup.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/bootstrap-datepicker.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/aos.js"></script>

    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/jquery.fancybox.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/swiper.min.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/jquery.scrollbar.js"></script>
    <script src="~/MvcOnlineTicariOtomasyon/thumber/js/main.js"></script>


</body>
</html>
