@model MvcOnlineTicariOtomasyon.Models.Sınıflar.Departman
@{
    ViewBag.Title = "DepartmanEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>DepartmanEkle</h2>

<br />
<form style="margin:20px;" class="form-group" method="post">
    @Html.LabelFor(x => x.DepartmanAdi)
    @Html.TextBoxFor(x => x.DepartmanAdi, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

